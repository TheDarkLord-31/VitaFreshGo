
entrypoint = "vite.config.js"
modules = ["nodejs-20"]

[nix]
channel = "stable-25_05"
packages = ["nodejs-20"]

[deployment]
run = ["npm", "run", "dev"]
deploymentTarget = "none"
ignorePorts = false


[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 3002
externalPort = 3002





